@model Skill
@{
    ViewData["Title"] = "CreateSkill";
    Layout = "~/Views/Adminlayout/Index.cshtml";
}

<h2>Yeni Yetenek Girişi</h2>

  <form method="post">
      <label>Yetenek Başlığı</label>
    <input type="text" asp-for="SkillName" class="form-control" />
      <br />
      <label>Yetenek Değeri</label>
      <input type="number" asp-for="SkillValue" class="form-control"/>
      <br />
      <button class="btn btn-info">Yeteneği Ekle</button>

  
  </form>
